# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Firebase Functions URL
VITE_FIREBASE_FUNCTIONS_URL=https://us-central1-your_project_id.cloudfunctions.net

# Application Base URL (used in email notifications)
VITE_APP_BASE_URL=https://your_project.web.app

# Google Gemini AI Configuration (for self-help AI assistant)
VITE_GEMINI_API_KEY=your_gemini_api_key

# AI Self-Help Settings
VITE_ENABLE_AI_SELF_HELP=true
VITE_ENABLE_TOKEN_CACHE=false

# Algolia Search Configuration
VITE_ALGOLIA_APP_ID=your_algolia_app_id
VITE_ALGOLIA_SEARCH_KEY=your_algolia_search_only_key

# Push Notifications (VAPID Key)
VITE_VAPID_KEY=your_vapid_public_key

# Mailgun Configuration (optional - for email features)
VITE_MAILGUN_DOMAIN=your_mailgun_domain
